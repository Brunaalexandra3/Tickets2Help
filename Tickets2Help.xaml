<Window x:Class="Tickets2Help.Tickets2Help"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tickets2Help"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FFE8D4D0">
        <Label x:Name="lbl_ticket" Content="Ticket2Help" HorizontalAlignment="Center" Margin="0,65,0,0" VerticalAlignment="Top" FontSize="48" FontFamily="Bernard MT Condensed"/>
        <Button x:Name="btn_login" Content="Login" HorizontalAlignment="Left" Margin="149,0,0,0" VerticalAlignment="Center" Height="48" Width="190" Background="#FFFDE1B3" FontFamily="Bernard MT Condensed" FontSize="20" Click="btn_login_Click"/>
        <Button x:Name="btn_register" Content="Register" HorizontalAlignment="Left" Margin="149,282,0,0" VerticalAlignment="Top" Height="48" Width="190" Background="#FFFDE1B3" FontSize="20" FontFamily="Bernard MT Condensed" Click="btn_register_Click"/>
    </Grid>
</Window>
